@page
@model Freelancing.Pages.Account.LogoutModel
@{
    ViewData["Title"] = "Logout";
}

<div class="row justify-content-center">
    <div class="col-md-6">
        <div class="text-center">
            <h3>You have been logged out</h3>
            <p class="lead">Thank you for using the admin panel.</p>
            <a asp-page="/Index" class="btn btn-primary">Return to Home</a>
            <a asp-page="/Account/Login" class="btn btn-outline-secondary">Login Again</a>
        </div>
    </div>
</div>